<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN" monitorInterval="300" packages="red.psyche.log.shield.log4j23">

	<Properties>
	</Properties>
 	
	<appenders>
	
		<Console name="Console" target="SYSTEM_OUT">  
		  <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{1.} - %msg%n"/>  
		</Console>
	</appenders>

	<loggers>
		<!-- AsyncRoot -->
		<AsyncRoot level="INFO">
			<appender-ref ref="Console" />
		</AsyncRoot>
	</loggers>
	
</configuration>